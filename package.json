{
  "name": "ember-attacher",
  "version": "2.1.0",
  "description": "Tooltips and popovers for Ember.js apps",
  "keywords": [
    "ember-addon",
    "tooltips",
    "popovers"
  ],
  "repository": "https://github.com/kybishop/ember-attacher",
  "license": "MIT",
  "author": {
    "name": "Kyle Bishop",
    "email": "kybishop@gmail.com",
    "url": "http://kybishop.com"
  },
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "build": "ember build",
    "deploy": "ember build && ember github-pages:commit --message \"Deploy gh-pages from commit $(git rev-parse HEAD)\" && git push origin gh-pages:gh-pages",
    "release": "release-it",
    "lint:js": "eslint .",
    "lint": "eslint .",
    "start": "ember serve",
    "test": "ember test",
    "test:all": "ember try:each"
  },
  "dependencies": {
    "@floating-ui/dom": "~1.2.7",
    "babel-plugin-filter-imports": "^4.0.0",
    "broccoli-funnel": "^3.0.5",
    "ember-auto-import": "~2.6.3",
    "ember-classic-decorator": "~3.0.1",
    "ember-cli-babel": "^7.26.10",
    "ember-cli-htmlbars": "~6.2.0",
    "ember-cli-sass": "~11.0.1",
    "ember-decorators": "^6.1.1",
    "ember-in-element-polyfill": "~1.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.21.4",
    "@babel/eslint-parser": "^7.21.3",
    "@ember/optional-features": "^2.0.0",
    "@ember/string": "^3.0.1",
    "@ember/test-helpers": "~2.9.3",
    "@ember/test-waiters": "^3.0.2",
    "@glimmer/component": "^1.0.0",
    "@glimmer/tracking": "^1.0.0",
    "@html-next/flexi-default-styles": "^2.2.0",
    "@release-it-plugins/lerna-changelog": "^5.0.0",
    "broccoli-asset-rev": "^3.0.0",
    "ember-cli": "~3.28.5",
    "ember-cli-dependency-checker": "~3.3.1",
    "ember-cli-github-pages": "^0.2.2",
    "ember-cli-inject-live-reload": "^2.0.2",
    "ember-cli-sri": "^2.1.1",
    "ember-cli-terser": "^4.0.2",
    "ember-disable-prototype-extensions": "^1.1.3",
    "ember-export-application-global": "^2.0.1",
    "ember-load-initializers": "^2.1.2",
    "ember-maybe-import-regenerator": "^1.0.0",
    "ember-power-select": "~5.0.4",
    "ember-qunit": "~5.1.3",
    "ember-resolver": "~10.0.0",
    "ember-source": "~3.28.11",
    "ember-source-channel-url": "^3.0.0",
    "ember-svg-jar": "~2.4.2",
    "ember-truth-helpers": "^3.1.1",
    "ember-try": "~3.0.0-beta.1",
    "eslint": "~8.39.0",
    "eslint-plugin-decorator-position": "^5.0.2",
    "eslint-plugin-ember": "~11.5.2",
    "eslint-plugin-node": "^11.1.0",
    "flexi": "^2.1.0",
    "loader.js": "^4.7.0",
    "qunit": "~2.19.4",
    "qunit-dom": "~2.0.0",
    "release-it": "~15.10.1",
    "sass": "^1.62.1",
    "webpack": "~5.81.0"
  },
  "resolutions": {
    "ember-in-element-polyfill": "^1.0.1",
    "ember-maybe-in-element": "2.0.1"
  },
  "engines": {
    "node": ">= 12.*"
  },
  "ember": {
    "edition": "octane"
  },
  "ember-addon": {
    "configPath": "tests/dummy/config",
    "demoURL": "https://tylerturdenpants.github.io/ember-attacher/"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  },
  "release-it": {
    "plugins": {
      "@release-it-plugins/lerna-changelog": {
        "infile": "CHANGELOG.md"
      }
    },
    "git": {
      "tagName": "v${version}"
    },
    "github": {
      "release": true
    }
  }
}
