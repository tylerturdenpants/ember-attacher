{
  "name": "ember-attacher",
  "version": "1.1.0",
  "description": "Tooltips and popovers for Ember.js apps",
  "keywords": [
    "ember-addon",
    "tooltips",
    "popovers"
  ],
  "repository": "https://github.com/kybishop/ember-attacher",
  "license": "MIT",
  "author": {
    "name": "Kyle Bishop",
    "email": "kybishop@gmail.com",
    "url": "http://kybishop.com"
  },
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "build": "ember build",
    "deploy": "ember build && ember github-pages:commit --message \"Deploy gh-pages from commit $(git rev-parse HEAD)\" && git push origin gh-pages:gh-pages",
    "release": "release-it",
    "lint:js": "eslint .",
    "start": "ember serve",
    "test": "ember test",
    "test:all": "ember try:each"
  },
  "dependencies": {
    "babel-plugin-filter-imports": "^4.0.0",
    "babel6-plugin-strip-class-callcheck": "^6.0.0",
    "broccoli-funnel": "^2.0.1",
    "ember-cli-babel": "^7.18.0",
    "ember-cli-htmlbars": "^4.2.3",
    "ember-cli-sass": "^10.0.1",
    "ember-popper": "^0.11.1",
    "ember-truth-helpers": "^2.1.0",
    "sass": "^1.26.2"
  },
  "devDependencies": {
    "@ember/optional-features": "^1.3.0",
    "@html-next/flexi-config": "^2.2.0",
    "@html-next/flexi-default-styles": "^2.2.0",
    "@html-next/flexi-dsl": "^2.2.0",
    "babel-eslint": "^10.1.0",
    "broccoli-asset-rev": "^3.0.0",
    "ember-cli": "~3.16.0",
    "ember-cli-dependency-checker": "^3.2.0",
    "ember-cli-eslint": "^5.1.0",
    "ember-cli-fastboot": "^2.2.1",
    "ember-cli-github-pages": "^0.2.2",
    "ember-cli-inject-live-reload": "^2.0.2",
    "ember-cli-sri": "^2.1.1",
    "ember-cli-uglify": "^3.0.0",
    "ember-disable-prototype-extensions": "^1.1.3",
    "ember-export-application-global": "^2.0.1",
    "ember-font-awesome": "^4.0.0-rc.4",
    "ember-load-initializers": "^2.1.1",
    "ember-maybe-import-regenerator": "^0.1.6",
    "ember-power-select": "^3.0.6",
    "ember-qunit": "^4.6.0",
    "ember-raf-test-waiter": "^0.1.0",
    "ember-resolver": "^7.0.0",
    "ember-source": "~3.17.0",
    "ember-source-channel-url": "^2.0.1",
    "ember-try": "^1.4.0",
    "eslint-plugin-ember": "^7.9.0",
    "eslint-plugin-node": "^11.0.0",
    "flexi": "^2.1.0",
    "loader.js": "^4.7.0",
    "qunit-dom": "^1.1.0",
    "release-it": "^12.6.3",
    "release-it-lerna-changelog": "^1.0.3"
  },
  "engines": {
    "node": "10.* || >= 12.*"
  },
  "ember-addon": {
    "configPath": "tests/dummy/config",
    "demoURL": "https://kybishop.github.io/ember-attacher/"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  },
  "release-it": {
    "plugins": {
      "release-it-lerna-changelog": {
        "infile": "CHANGELOG.md"
      }
    },
    "git": {
      "tagName": "v${version}"
    },
    "github": {
      "release": true
    }
  }
}
